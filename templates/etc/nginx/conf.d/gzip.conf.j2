gzip  on;
gzip_http_version 1.0;
gzip_comp_level 2;
gzip_min_length 1100;
gzip_buffers     4 8k;
gzip_proxied any;
gzip_types
    text/plain
    text/html
    text/xml
    text/css
    text/javascript
    text/x-component
    application/xml
    application/xhtml+xml
    application/rss+xml
    application/javascript
    application/x-javascript
    application/x-font-ttf
    application/vnd.ms-fontobject
    application/json
    mage/svg+xml
    font/opentype
    font/truetype
    font/ttf
    font/eot
    font/otf;

gzip_static on;

gzip_proxied        expired no-cache no-store private auth;
gzip_disable        "MSIE [1-6]\.";
gzip_vary           on;
